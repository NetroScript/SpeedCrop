[sub_resource type="GDScript" id="GDScript_rap77"]
script/source = "extends SpinBox


# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	value = Application.crop_to_size.x
	Application.crop_size_changed.connect(func(size: Vector2i): value = size.x) 
	
	get_line_edit().context_menu_enabled = false
	
	value_changed.connect( 
		func(value : float):
			Application.crop_to_size.x = int(value)
	)

func _gui_input(event: InputEvent) -> void:
	# On Escape release focus if owned
	if event is InputEventKey:
		if event.keycode == KEY_ESCAPE and has_focus():
			release_focus()
"

