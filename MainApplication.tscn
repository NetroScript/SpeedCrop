[sub_resource type="GDScript" id="GDScript_vbuka"]
script/source = "extends TextureButton

@onready
var file_dialog : FileDialog = %InputData
@onready
var input_path : LineEdit = %InputPath

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	button_up.connect(
		func():
			if not file_dialog.visible:
				file_dialog.popup_centered()
	)
	
	file_dialog.dir_selected.connect(
		func(dir: String):
			input_path.text = dir
			Application.entry_path = dir
			release_focus()
	)

"

[sub_resource type="GDScript" id="GDScript_iyy15"]
script/source = "extends CheckButton


# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	
	button_pressed = Application.load_subdirectories
	Application.load_subdirectories_changed.connect(func(value: bool): button_pressed = value)
	
	toggled.connect(func(value: bool): Application.load_subdirectories = value)
	

func _gui_input(event: InputEvent) -> void:
	# On Escape release focus if owned
	if event is InputEventKey:
		if event.keycode == KEY_ESCAPE and has_focus():
			release_focus()
"

[sub_resource type="GDScript" id="GDScript_3bsag"]
script/source = "extends RichTextLabel


func _input(event: InputEvent) -> void:
	if event is InputEventKey:
		if not event.pressed:
			if event.keycode == KEY_F1:
				visible = !visible
"

